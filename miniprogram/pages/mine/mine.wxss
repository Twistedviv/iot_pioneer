/* pages/mine/mine.wxss */
.container{
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
}
.head-image{
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
    margin-top: 60rpx;
}
.menu-container{
    width: 750rpx;
    height: 70%;
    padding: 50rpx 30rpx 0;
    box-sizing: border-box;
    border-radius: 40rpx 40rpx 0 0;
    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1)
}
